{
  "manifest_version": 2,
  "name": "PiControl",
  "version": "0.1.0",
  "minimum_chrome_version": "28",
  "icons": {
    "16": "chrome_app/icon_16.png",
    "128": "chrome_app/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["chrome_app/start.js"]
    }
  },
  "permissions": ["webview", "identity", "storage", "https://petele-home-automation.appspot.com/*"]
}
